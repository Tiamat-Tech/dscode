{
  "name": "dscode",
  "version": "1.0.0",
  "description": "Docker starter project for Digital Ocean droplet",
  "main": "index.js",
  "scripts": {
    "cp": "docker cp objCouchDb:/opt/couchdb/etc/ ./",
    "pwd": "echo 'Example: openssl passwd -crypt `${PWD}`",
    "pwd2": "echo 'Example: echo -n `${PWD}` | openssl dgst -sha256",
    "scpup": "scp -F c:\\Users\\denbrige\\.ssh\\config -i C:\\Users\\denbrige\\.ssh\\id_rsa_bt -r d:\\docker\\teedy root@142.93.12.220:/root",
    "scpdn1": "scp -F c:\\Users\\denbrige\\.ssh\\config -i C:\\Users\\denbrige\\.ssh\\id_rsa_do1 -r root@142.93.12.220:/root/teedy d:\\docker",
    "scpdn2": "scp -F c:\\Users\\denbrige\\.ssh\\config -i C:\\Users\\denbrige\\.ssh\\id_rsa_do1 -r root@178.128.91.93:/root/couchdb d:\\docker",
    "scpup1": "scp -F c:\\Users\\denbrige\\.ssh\\config -i C:\\Users\\denbrige\\.ssh\\id_rsa_bt default.conf root@142.93.12.220:/root/nginx/",
    "scpup2": "scp -F c:\\Users\\denbrige\\.ssh\\config -i C:\\Users\\denbrige\\.ssh\\id_rsa_bt docker-compose.yml root@142.93.12.220:/root/",
    "scpdir": "scp -F c:\\Users\\denbrige\\.ssh\\config -i C:\\Users\\denbrige\\.ssh\\id_rsa_do1 -r docker-couchdb root@142.93.12.220:/root/",
    "ssh": "ssh -F C:\\Users\\denbrige\\.ssh\\config -i C:\\Users\\denbrige\\.ssh\\id_rsa_bt root@142.93.12.220",
    "build": "docker build -t image .",
    "up": "docker-compose up -d",
    "up-win": "docker-compose -f docker-compose-win.yml up -d",
    "clean": "docker-compose down --rmi all",
    "down": "docker-compose down",
    "down-win": "docker-compose -f docker-compose-win.yml down",
    "bash": "docker run -it image /bin/bash",
    "stop": "docker-compose stop",
    "start": "docker-compose start",
    "restart": "docker-compose restart",
    "ls": "docker image ls && docker container ls -a",
    "rm": "docker rm $(docker ps -a --filter 'exited=2' -q)",
    "sshkeygen": "gulp -f js/gulp.js sshkeygen",
    "sshmkswap": "gulp -f js/gulp.js sshmkswap",
    "sshinstall": "gulp -f js/gulp.js sshinstall",
    "sshdocker": "gulp -f js/gulp.js sshdocker",
    "testdb": "curl -s localhost:5984",
    "tfws": "cd tf && terraform workspace list && cd ..",
    "tfinit": "cd tf && terraform init && cd ..",
    "tfplan": "cd tf && terraform plan && cd ..",
    "tfapply": "cd tf && terraform apply && cd ..",
    "tfdestroy": "cd tf && terraform destroy && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dennislwm/dscode.git"
  },
  "keywords": [
    "dscode",
    "code-server",
    "doc",
    "digitalocean",
    "docker",
    "starter",
    "droplet"
  ],
  "author": "dennislwm",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/dennislwm/dscode/issues"
  },
  "homepage": "https://github.com/dennislwm/dscode#readme",
  "devDependencies": {
    "gulp": "^4.0.2",
    "readline-sync": "^1.4.10",
    "ssh2-promise": "^0.1.6"
  }
}